  "private": true,
  "main": "webpack.config.js",
  "scripts": {
    "test": "./node_modules/jest/bin/jest.js",
    "jake": "./node_modules/jake/bin/cli.js",
    "start": "webpack-dev-server --progress --hot --config webpack.dev.config.js",
    "lint": "eslint . --ext .js --ext .jsx --ext .ts --ext .tsx || true",
    "check-types": "tsc",
    "build": "FAST_BUILD=true webpack --config webpack.production.config.js --color --profile",
    "analyze-build": "ANALYZE_BUILD=true webpack --config webpack.production.config.js --color --profile",
    "compressed-build": "webpack --config webpack.production.config.js --color --profile",
    "prepublishOnly": "bin/prevent-publishment-on-npm-registry"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./node_modules/jest-enzyme/lib/index.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)?$": "babel-jest"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "bin/check-nocommit && bin/check-eslint && bin/check-stylelint"
    }
  },
